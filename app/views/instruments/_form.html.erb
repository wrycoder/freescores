<%= form_with model: @instrument do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :family %>
  <%= f.text_field :family %>
  <%= f.label :rank %>
  <%= f.select :rank,
    options_for_select(
      (10..2500).step(10).to_a.map{|s| ["#{s}", s]}) %>
  <%= f.submit %>
<% end %>

